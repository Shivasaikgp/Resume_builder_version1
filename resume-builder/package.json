{
  "name": "resume-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run src/__tests__/*integration*.test.ts",
    "test:ai": "vitest run src/__tests__/ai-agents-comprehensive.test.ts",
    "test:performance": "vitest run src/__tests__/performance/",
    "test:accessibility": "vitest run src/__tests__/accessibility/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test src/__tests__/visual/",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:visual",
    "test:comprehensive": "node scripts/run-comprehensive-tests.js",
    "test:ci": "npm run test:unit -- --coverage && npm run test:e2e -- --reporter=github",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "deploy": "node scripts/deploy.js deploy --production",
    "deploy:check": "node scripts/deploy.js check",
    "deploy:staging": "node scripts/deploy.js deploy",
    "backup:create": "node scripts/backup.js create",
    "backup:restore": "node scripts/backup.js restore",
    "backup:list": "node scripts/backup.js list",
    "backup:cleanup": "node scripts/backup.js cleanup",
    "docker:build": "docker build -t resume-builder .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "monitoring:up": "docker-compose --profile monitoring up -d",
    "monitoring:down": "docker-compose --profile monitoring down"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@nextui-org/react": "^2.4.2",
    "@nextui-org/system": "^2.2.2",
    "@nextui-org/theme": "^2.2.6",
    "@prisma/client": "^5.15.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@types/multer": "^2.0.0",
    "@radix-ui/react-icons": "^1.3.0",
    "ai": "^4.3.19",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "framer-motion": "^12.23.6",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.6.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "mammoth": "^1.9.1",
    "multer": "^2.0.2",
    "next": "15.4.1",
    "next-auth": "^4.24.11",
    "openai": "^5.10.1",
    "p-queue": "^8.1.0",
    "p-retry": "^6.2.1",
    "pdf-parse": "^1.1.1",
    "prisma": "^6.12.0",
    "puppeteer": "^24.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.60.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.5",
    "zustand": "^5.0.6",
    "autoprefixer": "^10.4.19"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2"
  },
  "ct3aMetadata": {
    "title": "Resume Builder",
    "description": "Build your resume with ease and confidence. This application allows you to create, edit, and preview your resume in various formats. It's built with Next.js, Prisma, and Tailwind CSS."
  }
}
